#!/usr/bin/env bash

# bunyan-postgres-stream
P='
adds
airport-diagrams
chart-supplements
coverage-viewer
deedee
node-http-client-bench
notams
papergen
papergen-cli
paper-rulings
proxycache
terminal-procedures
wikiquote
wikiquote-cli
rvr
postdoc
'

for package in $P; do
  cd ~/Workspace/github/fdesjardins/$package
  ncu -a
  npm i
  yarn
  git add .
  git commit -m "update dependencies"
  np patch
done
